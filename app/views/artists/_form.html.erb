<%= simple_form_for(@artist) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :username %>
    <%= f.input :description %>
    <%= f.input :domain_id, collection: Domain.all %>
    <%= f.input :street %>
    <%= f.input :zipcode %>
    <%= f.input :city %>
    <%= f.input :country, as: :string %>
    <%= f.input :phone %>
    <%= f.input :siret %>
    <%= f.input :website %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
